# MANAGED IN ANSIBLE, DO NOT MODIFY
#
# Custom Praxis Nagios config

{% for host in groups['nagios-servers'] %}
allowed_hosts={{ hostvars[host]['ansible_ssh_host'] }}
{% endfor %}

command[check_load_custom]=/usr/lib/nagios/plugins/check_load -w 5 -c 10
command[check_disk_custom]=/usr/lib/nagios/plugins/check_disk -w 20% -c 10% -p /
